<script type="text/javascript">
function showDialog(id){
    var dialog = $(id).data('dialog');
    dialog.open();
    $("#login-please a").click(function() {
      if (!$(".app-bar-element form input").length)
        return true;
      $("#login-please").hide();
      $(".app-bar-element > a").last().click();
      return false;
    });
}
$(document).ready(function() {
  $(".dialog-close-button").click(function() {
    $("#login-please").hide();
    $.cookie('timer1', 0);
  });
  var max = 60000 * 4;
  if (intval($.cookie('timer1')) < max) {

    var interval = setInterval(function() {
      $.cookie('timer1', intval($.cookie('timer1')) + 1000);
      if (intval($.cookie('timer1')) >= max) {
        clearInterval(interval);
        showDialog('#login-please');
      } else 
        console.log($.cookie('timer1'));
    }, 1000);
  } else 
    showDialog('#login-please');
})
  
</script>
